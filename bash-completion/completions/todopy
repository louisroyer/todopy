#/usr/bin/env bash
_todopy_completions()
{
	case "${COMP_WORDS[1]}" in
		edit|create|ls|ll|help|directory|push) COMPREPLY=();;
		commit)
			case "${COMP_WORDS[2]}" in
				all) COMPREPLY=();;
				*)
				COMPREPLY=($(compgen -W "'all'" "${COMP_WORDS[2]}"));;
			esac
			;;
	*) COMPREPLY=($(compgen -W "edit commit create ls ll help directory push" "${COMP_WORDS[1]}"));;
	esac
}
complete -F _todopy_completions todopy
